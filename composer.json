{
    "name": "2amigos/yii2-type-ahead-widget",
    "description": "The bootstrap typeahead widget for the Yii framework",
    "keywords": [
        "2amigos",
        "yii",
        "yii2",
        "yii 2",
        "widget",
        "typeahead"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/2amigos/yii2-type-ahead-widget",
    "authors": [
        {
            "name": "Alexander Kochetov",
            "email": "creocoder@gmail.com",
            "role": "Developer"
        },
        {
            "name": "2amigOS! Consulting Group",
            "email": "hola@2amigos.us",
            "homepage": "http://2amigos.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "yiisoft/yii2": "~2.0.15",
        "yiisoft/yii2-bootstrap": "~2.0.8",
        "bower-asset/typeahead.js": "~0.11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\typeahead\\": "src"
        }
    },
    "config": {
        "process-timeout": 7200,
        "optimize-autoloader": true,
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
